{
  "name": "limited-cache",
  "description": "A minimal JS cache: like using an object, except it won't grow forever",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Steven Pautz <spautz@gmail.com>",
  "homepage": "https://github.com/spautz/limited-cache",
  "repository": "github:spautz/limited-cache",
  "main": "lib-cjs/redux.js",
  "module": "dist-es/limited-cache.mjs",
  "unpkg": "dist-umd/redux.js",
  "types": "types/index.d.ts",
  "files": [
    "dist-cjs",
    "dist-es",
    "dist-umd",
    "src",
    "types"
  ],
  "scripts": {
    "____ BASE COMMANDS _________________________________________________": "",
    "clean": "rimraf dist-* coverage types",
    "build": "rollup -c",
    "format": "prettier --write \"**/*.{js,json,md,ts}\"",
    "lint": "eslint \"**/*.{js,ts}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "types": "tsc --noEmit src/index.ts",
    "____ INTEGRATION ___________________________________________________": "",
    "dev": "yon run format && yon run types && yon run lint",
    "prepare": "yon run clean && yon run dev && yon test && yon build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.9",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2",
    "yarn-or-npm": "^3.0.1"
  },
  "optionalDependencies": {},
  "peerDependencies": {},
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|/tests/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "lint-staged": {
    "**/*.{css,html,js,json,less,md,scss,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
