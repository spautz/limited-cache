{
  "name": "limited-cache",
  "description": "A minimal JS cache: like using an object, except it won't grow forever",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Steven Pautz <spautz@gmail.com>",
  "homepage": "https://github.com/spautz/limited-cache",
  "repository": "github:spautz/limited-cache",
  "main": "dist-cjs/index.js",
  "module": "dist-es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist-cjs",
    "dist-es",
    "src",
    "types"
  ],
  "scripts": {
    "____ BASE COMMANDS _________________________________________________": "",
    "clean": "rimraf dist-*/ coverage/ types/ ; jest --clearCache",
    "build": "rollup -c",
    "format": "prettier --write \"**/*.{js,json,md,ts}\"",
    "lint": "eslint \"**/*.{js,ts}\"",
    "prepare": "yon run clean",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "types": "tsc --noEmit index.ts",
    "____ INTEGRATION ___________________________________________________": "",
    "dev": "yon run format && yon run types && yon run lint",
    "all": "yon run dev && yon run test:coverage && yon run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4",
    "yarn-or-npm": "^3.0.1"
  },
  "optionalDependencies": {},
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|/tests/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "lint-staged": {
    "**/*.{css,html,js,json,less,md,scss,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
