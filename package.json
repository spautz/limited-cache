{
  "name": "limited-cache",
  "version": "0.5.1",
  "description": "A minimal JS cache: like using an object, except it won't grow forever",
  "keywords": [
    "cache",
    "limited cache",
    "circular buffer",
    "rotating cache",
    "max size",
    "autoexpire"
  ],
  "license": "MIT",
  "homepage": "https://github.com/spautz/limited-cache#readme",
  "repository": "https://github.com/spautz/limited-cache",
  "bugs": "https://github.com/spautz/limited-cache/issues",
  "author": "Steven Pautz <spautz@gmail.com>",
  "files": [
    "dist/",
    "src/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "____ LIFECYCLE HOOKS _______________________________________________": "",
    "prepare": "yon run build",
    "prebuild": "yon run build:clean",
    "prerelease": "yon run build:clean",
    "prepublishOnly": "yarn run checkup && yarn run build",
    "pretest": "yon run test:clean",
    "postbuild": "yon run build:checkup",
    "____ INTEGRATION ___________________________________________________": "",
    "dev": "yon run format && yon run types && yon run lint",
    "checkup": "yon run format:checkup && yon run types && yon run lint",
    "clean": "yon run build:clean && yon run test:clean && rimraf dist/ node_modules/.cache/",
    "ci": "yon run clean && yon run checkup && yon run test:report",
    "all": "yon run clean && yon run dev && yon run test && yon run build",
    "____ INDIVIDUAL COMMANDS ___________________________________________": "",
    "build": "tsdx build",
    "build:clean": "rimraf dist/",
    "build:checkup": "node ./scripts/verify-build.js",
    "build:watch": "tsdx build --watch",
    "format": "prettier --write \"**/*.*\"",
    "format:checkup": "prettier --list-different \"**/*.*\"",
    "lint": "tsdx lint .",
    "release:changelog": "standard-version --skip.commit --skip.tag --release-as ",
    "release:tag": "standard-version --commit-all --sign --skip.changelog --release-as ",
    "test": "tsdx test --coverage",
    "test:clean": "rimraf coverage/",
    "test:report": "yon run test --coverageReporters=text-lcov | coveralls",
    "test:watch": "yon run test --watch",
    "types": "tsc --noEmit --p tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "coveralls": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "yarn-or-npm": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "lint-staged": {
    "**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run checkup && yarn run test"
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "yon run format"
    }
  }
}
