{
  "name": "limited-cache",
  "version": "2.0.0",
  "description": "A minimal JS cache: like using an object, except it won't grow forever",
  "keywords": [
    "limited cache",
    "cache",
    "circular buffer",
    "max size",
    "max time",
    "rotating cache",
    "offline cache",
    "local storage",
    "autoexpire"
  ],
  "license": "MIT",
  "homepage": "https://github.com/spautz/limited-cache#readme",
  "bugs": "https://github.com/spautz/limited-cache/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/spautz/limited-cache.git"
  },
  "author": {
    "name": "Steven Pautz",
    "url": "http://stevenpautz.com/"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "files": [
    "dist/",
    "legacy-types/",
    "src/",
    "LICENSE",
    "*.md"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": "^14 || ^16 || ^18 || ^19 || ^20",
    "pnpm": "^8"
  },
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "____ LIFECYCLE HOOKS _______________________________________________": "",
    "prepare": "pnpm run build && husky install",
    "prerelease": "pnpm run build:clean",
    "prepublishOnly": "pnpm run dev:readonly && pnpm run build",
    "____ INTEGRATION ___________________________________________________": "",
    "clean": "pnpm run build:clean && pnpm run test:clean && rimraf ./node_modules/.cache/ *.log",
    "dev": "pnpm run format && pnpm run types && pnpm run lint",
    "dev:readonly": "pnpm run format:verify && pnpm run types && pnpm run lint",
    "all": "pnpm run clean && pnpm run dev && pnpm run test:coverage && pnpm run build",
    "all:readonly": "pnpm run dev:readonly && pnpm run test:coverage",
    "____ ALIASES _______________________________________________________": "",
    "test": "pnpm run test:coverage",
    "____ INDIVIDUAL COMMANDS ___________________________________________": "",
    "build": "pnpm run build:clean && pnpm run build:main && pnpm run build:verify && pnpm run build:types",
    "build:clean": "rimraf ./dist ./legacy-types",
    "build:main": "tsdx build",
    "build:types": "pnpm run build:types:3.x && pnpm run build:types:4.x",
    "build:types:3.x": "downlevel-dts ./dist ./legacy-types/ts3.x/dist --to=3.0",
    "build:types:4.x": "downlevel-dts ./dist ./dist --to=4.0",
    "build:verify": "node scripts/verify-build.cjs",
    "build:watch": "tsdx watch",
    "format": "prettier --write \"**/*.*\"",
    "format:verify": "prettier --list-different \"**/*.*\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "lint-staged": "lint-staged",
    "release:changelog": "standard-version --skip.commit --skip.tag --release-as ",
    "release:tag": "standard-version --commit-all --sign --skip.changelog --release-as ",
    "test:clean": "rimraf ./coverage",
    "test:coverage": "pnpm run test:clean      && vitest run --coverage",
    "test:quick": "pnpm run test:clean         && vitest run --coverage=false",
    "test:watch": "pnpm run test:clean         && vitest watch --coverage=false",
    "test:watchcoverage": "pnpm run test:clean && vitest watch --coverage",
    "types": "tsc --noEmit --p tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitest/coverage-c8": "0.30.1",
    "downlevel-dts": "0.11.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "tsdx": "0.14.1",
    "typescript": "5.0.4",
    "vitest": "0.30.1"
  },
  "lint-staged": {
    "**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx}": [
      "prettier --write"
    ]
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "pnpm run format"
    }
  },
  "typesVersions": {
    "<4": {
      "*": [
        "legacy-types/ts3.x/*"
      ]
    }
  }
}
