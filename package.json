{
  "name": "limited-cache",
  "version": "0.5.0",
  "description": "A minimal JS cache: like using an object, except it won't grow forever",
  "keywords": [
    "cache",
    "limited cache",
    "circular buffer",
    "rotating cache",
    "max size",
    "autoexpire"
  ],
  "license": "MIT",
  "homepage": "https://github.com/spautz/limited-cache#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spautz/limited-cache.git"
  },
  "bugs": {
    "url": "https://github.com/spautz/limited-cache/issues"
  },
  "author": "Steven Pautz <spautz@gmail.com>",
  "files": [
    "dist/",
    "hooks/",
    "src/",
    "index.cjs.js",
    "LICENSE",
    "README.md"
  ],
  "source": "src/index.ts",
  "main": "index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "____ LIFECYCLE HOOKS _______________________________________________": "",
    "prepare": "yon run build",
    "prepublishOnly": "yon run verify && yon run build",
    "prebuild": "yon run build:clean",
    "postbuild": "yon run build:verify",
    "pretest": "yon run test:clean",
    "____ BATCH COMMANDS ________________________________________________": "",
    "clean": "yon build:clean && yon run test:clean",
    "stage": "yon run format         && yon run types && yon run lint",
    "verify": "yon run format:verify && yon run types && yon run lint",
    "all": "yon run clean && yon run stage  && yon run test:coverage && yon run build",
    "ci": "yon run clean  && yon run verify && yon run test:report   && yon run build",
    "____ INDIVIDUAL COMMANDS ___________________________________________": "",
    "build": "rollup -c",
    "build:clean": "rimraf dist/",
    "build:verify": "node ./scripts/verify-build.js",
    "format": "prettier --write \"**/*.*\"",
    "format:verify": "prettier --list-different \"**/*.*\"",
    "lint": "eslint \"**/*.{js,jsx,json,ts,tsx}\"",
    "release:changelog": "standard-version --sign --release-as ",
    "test": "jest",
    "test:clean": "rimraf coverage/",
    "test:coverage": "jest --coverage",
    "test:report": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "types": "tsc --noEmit --p tsconfig.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "2.21.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard-version": "^8.0.1",
    "ts-jest": "^26.1.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6",
    "yarn-or-npm": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|/tests/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "dist/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "lint-staged": {
    "**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run verify && yarn run test"
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "yon run format"
    }
  }
}
